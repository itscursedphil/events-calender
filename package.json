{
  "name": "events-calendar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "lerna run clean && lerna clean --yes",
    "prepare": "npm run clean && lerna bootstrap",
    "develop": "lerna run develop --stream --parallel",
    "develop:backend": "lerna run --scope @events-calendar/backend develop --stream",
    "develop:frontend": "lerna run --scope @events-calendar/frontend develop --stream",
    "start": "lerna run start --stream --parallel",
    "start:backend": "lerna run --scope @events-calendar/backend start --stream",
    "start:frontend": "lerna run --scope @events-calendar/frontend start --stream",
    "build": "lerna run build --stream --parallel",
    "build:backend": "lerna run --scope @events-calendar/backend build --stream",
    "build:frontend": "lerna run --scope @events-calendar/frontend build --stream",
    "update-dependencies": "npm-check -u && npm-check -u packages/backend && npm-check -u packages/frontend"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@t3n/eslint-config": "^6.0.2",
    "@t3n/eslint-config-react": "^6.0.2",
    "@t3n/eslint-config-typescript": "^6.0.2",
    "lerna": "^5.5.1",
    "npm-check": "^6.0.1",
    "typescript": "^4.8.3"
  }
}
